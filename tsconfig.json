{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "ESNext",
    "strict": true,
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
//    "moduleResolution": "node",
//    use that for npx @rtk-query/codegen-openapi openapi-config.ts
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "exclude": ["node_modules", "babel.config.js", "eslint.config.mjs", ".prettierrc.cjs"]
}
